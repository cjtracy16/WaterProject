@{
}

@model IEnumerable<string>

<div class="d-grid gap-2">
    <div class="mb-5">
        <h4>Project Types:</h4>
    </div>
    
    <a asp-controller="Home" asp-action="Index" asp-route-projectType="" asp-route-pageNum ="1" class="btn btn-outline-secondary">Home</a><br />


    @foreach (string t in Model)
    {
        <a asp-controller="Home" asp-action="Index" asp-route-projectType="@t" asp-route-pageNum ="1" 
           class="btn myBtn @(ViewBag.SelectedType == t ? "btn-primary" : "btn-outline-secondary")">@t</a><br />
    }
</div>

